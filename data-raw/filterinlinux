# Filter csv file by value 

zcat ~/ce_combinednomenclature_unlogged_2014.csv.gz | head | awk -F , '{ if ($1 == 02) print $2, $4 }'

cut -d , -f 2,4-6 input.csv
cut -f1-5,10-20 data.txt

awk '{ if ($4 >= 1 && $4 <= 10) print $1 }' sample.txt

bzcat input.csv.bz2 | cut -d , -f 3 | bzip2 > col3.csv.bz2

